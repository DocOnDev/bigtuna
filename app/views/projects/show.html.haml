%h2= @project.name
= link_to("Edit", edit_project_path(@project))
%br
%span= @project.vcs_type
%span= @project.vcs_source

= link_to("Build now", build_project_path(@project))

%ol
  - @builds.each do |build|
    %li
      %strong= link_to("#{build.commit[0, 10]} @ #{build.created_at}", build_path(build))
      %br
      %strong= build.status
